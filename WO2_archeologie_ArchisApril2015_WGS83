        "B_WOONPL": "",
        "DATUM_B": 1947,
        "TOPONIEM": "N.H. KERK (VM. ST. JOHANNES DE DOPERKERK)",
        "PLAATS": "Rinsumageest",
        "GEMEENTE": "Dantumadiel",
        "PROVINCIE": "Friesland",
        "VERWERV": "AOP",
        "MAAIVELD": "BEB",
        "GEOMORF": "XXX",
        "TEXTUUR": "KLEI",
