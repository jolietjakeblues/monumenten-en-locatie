        "B_PID": "mtump",
        "B_NAAM": "Tump",
        "B_WOONPL": "",
        "DATUM_B": "25-5-2009",
        "TOPONIEM": "Kreijtenberg",
        "PLAATS": "Milheeze",
        "GEMEENTE": "Gemert-Bakel",
        "PROVINCIE": "Noord-Brabant",
